<view class="container">
    <view class="page-body">
        <button bind:tap="chooseImage" type="primary" hover-stop-propagation="true">点击选择图片</button>
        <button bind:tap="previewImage" type="primary" hover-stop-propagation="true">点击预览图片</button>
    </view>
    <view class="page-title">
        <view class="page-title-line"></view>
        <view class="page-title-text">{{title}}</view>
    </view>
</view>